    "@sentry/node": "^5.0.8",
    "aws-cloudfront-sign": "^2.2.0",
    "aws-sdk": "^2.437.0",
    "body-parser": "^1.18.3",
    "connect-pg-simple": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "date-fns": "2.0.0-alpha.26",
