{
  "name": "simple",
  "version": "1.0.0",
  "description": "Simple boilerplate",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "BABEL_ENV=production NODE_ENV=production yarn webpack --mode production --config webpack.prod.js",
    "migrate": "./scripts/migrateAndSeed.sh",
    "start": "yarn migrate && DEBUG=knex*,-knex:client,-knex:connection,-knex:pool nodemon --inspect=0.0.0.0 .",
    "start:prod": "yarn migrate && yarn build && NODE_ENV=production node .",
    "precommit": "lint-staged && npm run test"
  },
  "author": "",
  "license": "ISC",
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 75.5,
        "branches": 41.5,
        "functions": 52.5,
        "lines": 77
      }
    },
    "transform": {
      "^.+\\.jsx?$": "babel7-jest"
    },
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
